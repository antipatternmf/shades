.temp {
  &__outer {
    display: flex;
    justify-content: center;
    height: 300px;
  }

  &__inner {
    display: flex;
    align-items: center;
    border: 1px solid blue;
    width: 300px;
  }

  &__text {
    flex-grow: 1;
    text-align: center;
    font-weight: bold;
    color: #171717;
  }
}
